#include <iostream>

using namespace std;

//Just like objects and object oriented design!
struct Student {
  char name[10];
  int id;
  float gpa;
};

//Main function of program
int main() {
  Student george;
  cout << "What is George's name?" << endl;
  cin >> george.name;
  george.id = 1234;
  george.gpa = 3.0;
  cout << "Name: " << george.name << ", ID: " << george.id << ", GPA: " george.gpa << endl;
}
